{
  "name": "recipes",
  "version": "0.1.1",
  "description": "Recipe app",
  "main": "index.js",
  "scripts": {
    "build": "babel server -d dist",
    "lint": "esw \"./**/*.js\" --color",
    "lint:watch": "npm run lint -- --watch",
    "postinstall": "npm run build",
    "start": "nodemon server/index.js --exec babel-node",
    "serve": "node dist/index.js",
    "test": "mocha --require babel-polyfill --compilers js:babel-register --reporter spec \"./server/**/*.test.js\" \"./tests/**/*.test.js\""
  },
  "engines": {
    "node": "6.1.0",
    "npm": "3.10.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vjwilson/recipes.git"
  },
  "keywords": [
    "recipes",
    "api",
    "javascript",
    "es2015"
  ],
  "author": "Van J. Wilson <van.wilson1992@alumni.unc.edu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vjwilson/recipes/issues"
  },
  "homepage": "https://github.com/vjwilson/recipes#readme",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "bcrypt": "^1.0.1",
    "body-parser": "^1.15.2",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "pg": "^6.1.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.11.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-watch": "^2.1.14",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "sinon": "^1.17.6",
    "supertest": "^2.0.1"
  }
}
